			"menuTransitionAnimation": [{
				"animationDuration": 300,
				"animationEasingCurve": "Linear"
			}],
			"floatPosition": [
				{
					"relativeTo": "centralContenedor",
					"position": "center-center",
					"shadow":[
						{
							"color": "#f00",
							"blurRadius": 30,
							"xOffset": 0,
							"yOffset": 0
