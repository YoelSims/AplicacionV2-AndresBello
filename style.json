			"expandedSize": [{
				"width": 280,
				"height": "parent"
			}],
			"menuTransitionAnimation": [{
				"animationDuration": 350,
				"animationEasingCurve": "Linear"
			}]
		},
		{
			"name": "menuContenedorDerecho",
			"defaultSize": [{
				"width": 0,
				"height": "parent"
			}],
			"collapsedSize": [{
				"width": 0,
				"height": "parent"
